            "from": {
                "data": "tree"
            },
            "encode": {
                "enter": {
                    "text": {
                        "field": "id"
                    },
                    "fontSize": {
                        "value": 9
                    },
                    "baseline": {
                        "value": "middle"
                    }
                },
                "update": {
                    "x": {
                        "field": "x"
                    },
                    "y": {
                        "field": "y"
                    },
                    "dx": {
                        "signal": "(datum.leftside ? -1 : 1) * 6"
                    },
                    "angle": {
                        "signal": "datum.leftside ? datum.angle - 180 : datum.angle"
                    },
                    "align": {
                        "signal": "datum.leftside ? 'right' : 'left'"
                    },
                    "opacity": {
                        "signal": "labels ? 1 : 0"
                    }
                }
            }
        }
    ]
}